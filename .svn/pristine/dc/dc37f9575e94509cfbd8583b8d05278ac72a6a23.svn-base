package kr.or.plzdvl.dvrTest.vo;

public class QuizOptVO {
	private String opt_num;
	private String opt_con;
	private String quiz_num;
	private String opt_wht;
	
	public String getOpt_num() {
		return opt_num;
	}
	public void setOpt_num(String opt_num) {
		this.opt_num = opt_num;
	}
	public String getOpt_con() {
		return opt_con;
	}
	public void setOpt_con(String opt_con) {
		this.opt_con = opt_con;
	}
	public String getQuiz_num() {
		return quiz_num;
	}
	public void setQuiz_num(String quiz_num) {
		this.quiz_num = quiz_num;
	}
	public String getOpt_wht() {
		return opt_wht;
	}
	public void setOpt_wht(String opt_wht) {
		this.opt_wht = opt_wht;
	}	
	
}
