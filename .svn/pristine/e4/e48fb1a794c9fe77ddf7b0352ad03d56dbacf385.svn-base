package kr.or.plzdvl.notice.vo;

public class NoticeVO {
	
	private String nbod_num; // 공지사항 번호
	private String nbod_tit; // 공지사항 제목
	private String nbod_con; // 공지사항 내용
	private String nbod_inst_date; // 공지사항 등록날짜
	private String mem_id; // 등록자
	private String nbod_wht; // 삭제여부
	
	
	public String getNbod_num() {
		return nbod_num;
	}
	public void setNbod_num(String nbod_num) {
		this.nbod_num = nbod_num;
	}
	public String getNbod_tit() {
		return nbod_tit;
	}
	public void setNbod_tit(String nbod_tit) {
		this.nbod_tit = nbod_tit;
	}
	public String getNbod_con() {
		return nbod_con;
	}
	public void setNbod_con(String nbod_con) {
		this.nbod_con = nbod_con;
	}
	public String getNbod_inst_date() {
		return nbod_inst_date;
	}
	public void setNbod_inst_date(String nbod_inst_date) {
		this.nbod_inst_date = nbod_inst_date;
	}
	public String getMem_id() {
		return mem_id;
	}
	public void setMem_id(String mem_id) {
		this.mem_id = mem_id;
	}
	public String getNbod_wht() {
		return nbod_wht;
	}
	public void setNbod_wht(String nbod_wht) {
		this.nbod_wht = nbod_wht;
	}
	
	
}
